class Solution {
public:
    int maxArea(vector<int>& height) {
        int n=height.size();
        int i=0,j=n-1;
        int maxA=INT_MIN;

        while(i<j){
            int h=min(height[i],height[j]);
            int w=j-i;
            maxA=max(maxA, (h*w));
            if(height[i]<height[j]){
                i++;
            }else
                j--;
            
        }
        return maxA;
    }
};
